<%= form_with(url: search_events_path, local: true, method: :get, class: "search-form") do |f| %>
  <div class="input-group no-border">
    <%= f.text_field :keyword, class: "form-control search-control", placeholder: "Search..." %>
    <%= button_tag type: "submit", class: "btn btn-outline-secondary search-btn " do %>
    <i class="fas fa-search"></i>
    <% end %>
  </div>
<% end %>