<%= form_with model: group, url: event_groups_path(event), method: :post, local: true do |f| %>
  <div class="form-group">
    <%= f.label :グループ名 %>
    <%= f.text_field :name, class: 'form_control' %>
  </div>
  
  <div class="form-group">
    <%= f.submit class: 'btn btn-info' %>
  </div>
<% end %>